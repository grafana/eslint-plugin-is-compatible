{
  "name": "@grafana/eslint-plugin-is-compatible",
  "version": "0.0.2",
  "description": "An ESlint plugin for Grafana plugins to check compatibility with Grafana versions",
  "main": "./dist/index.js",
  "files": [
    "docs/",
    "lib/",
    "dist/"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "type": "commonjs",
  "exports": {
    ".": {
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "grafana"
  ],
  "homepage": "https://www.npmjs.com/package/@grafana/eslint-plugin-is-compatible",
  "repository": {
    "type": "git",
    "url": "https://github.com/grafana/eslint-plugin-is-compatible"
  },
  "bugs": "https://github.com/grafana/eslint-plugin-is-compatible/issues",
  "scripts": {
    "build": "tsc --build",
    "build:diagnostics": "tsc --build --diagnostics",
    "watch": "tsc --watch",
    "tsup": "tsup ./lib",
    "tsup:watch": "tsup ./lib --watch",
    "docs:init": "eslint-doc-generator --init-rule-docs",
    "docs:update": "eslint-doc-generator",
    "test": "vitest"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@esm2cjs/execa": "^6.1.1-cjs.1",
    "@grafana/levitate": "^0.15.0",
    "@typescript-eslint/utils": "^7.13.0",
    "eslint": "^8.56.0",
    "execa": "^9.5.1",
    "node-fetch-commonjs": "^3.3.2",
    "semver": "^7.6.3",
    "tar": "^7.0.1",
    "typescript": "5.6.3"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.10",
    "@typescript-eslint/parser": "^7.13.0",
    "@typescript-eslint/rule-tester": "^7.13.0",
    "eslint-doc-generator": "^1.7.1",
    "tsup": "^8.1.0",
    "vitest": "^1.6.0"
  }
}
